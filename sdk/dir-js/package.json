    "lint": "gts lint",
    "fix": "gts fix",
    "pretest": "npm run build"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.8.0",
    "@connectrpc/connect": "^2.1.0",
    "@connectrpc/connect-node": "^2.1.0",
    "@grpc/grpc-js": "^1.13.4"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "gts": "^6.0.2",
    "typescript": "^5.6.3",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@buf/agntcy_dir.bufbuild_es": "^2.8.0-20250915154105-152c4b7b4ec6.1"
  },
  "peerDependenciesMeta": {
    "@buf/agntcy_dir.bufbuild_es": {
      "optional": false
    }
  },
  "vitest": {
    "test": {
      "globals": true,
      "environment": "node",
      "include": [
        "test/**/*.test.ts"
      ]
    }
  }
}
